category:            Foreign
build-type:          Simple
extra-source-files:  ChangeLog.md
cabal-version:       >=1.10

Executable test
  default-language:    Haskell2010
  hs-source-dirs:      app
  Main-Is:             Main.hs
  build-depends:       base >=4.9 && <4.10
                     , pblib-haskell
                     , transformers

library
  exposed-modules:     SAT.PseudoBoolean
